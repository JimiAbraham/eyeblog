        <div class="nk-apps-sidebar is-theme">
            <div class="nk-apps-brand">
                <a href="https://dashlite.net/demo3/index.html" class="logo-link"><img class="logo-light logo-img" src="https://dashlite.net/demo3/images/logo-small.png" srcset="/demo3/images/logo-small2x.png 2x" alt="logo"><img class="logo-dark logo-img" src="https://dashlite.net/demo3/images/logo-dark-small.png" srcset="/demo3/images/logo-dark-small2x.png 2x"
                        alt="logo-dark"></a>
            </div>


            <div class="nk-sidebar-profile nk-sidebar-profile-fixed dropdown">
                <a href="#" class="toggle" data-target="profileDD">
                    <div class="user-avatar"><span>AB</span></div>
                </a>
                <div class="dropdown-menu dropdown-menu-md m-1 nk-sidebar-profile-dropdown" data-content="profileDD">
                    <div class="dropdown-inner user-card-wrap d-none d-md-block">
                        <div class="user-card">
                            <div class="user-avatar"><span>AB</span></div>
                            <div class="user-info"><span class="lead-text">Abu Bin Ishtiyak</span><span class="sub-text text-soft">info@softnio.com</span></div>
                        </div>
                    </div>
                    <div class="dropdown-inner">
                        <ul class="link-list">
                            <li><a href="https://dashlite.net/demo3/user-profile-regular.html"><em class="icon ni ni-user-alt"></em><span>View Profile</span></a></li>
                            <li><a href="https://dashlite.net/demo3/user-profile-setting.html"><em class="icon ni ni-setting-alt"></em><span>Account Setting</span></a></li>
                            <li><a href="https://dashlite.net/demo3/user-profile-activity.html"><em class="icon ni ni-activity-alt"></em><span>Login Activity</span></a></li>
                        </ul>
                    </div>
                    <div class="dropdown-inner">
                        <ul class="link-list">
                            <li><a href="#"><em class="icon ni ni-signout"></em><span>Sign out</span></a></li>
                        </ul>
                    </div>
                </div>
            </div>


        </div>

        <div class="nk-main ">
            <div class="nk-wrap ">
                <div class="nk-header nk-header-fixed is-light">
                    <div class="container-fluid">

                    </div>
                </div>
                <!-- navbar -->

                <%- include('./includes/sideNav') -%>

                    <!-- navbar -->
                    <div class="nk-content ">
                        <div class="container-fluid">
                            <div class="nk-content-inner">
                                <div class="nk-content-body">
                                    <div class="nk-block-head nk-block-head-sm">
                                        <div class="nk-block-between pos-rel">
                                            <div class="nk-block-head-content">
                                                <h3 class="nk-block-title page-title">Newly Added Article</h3>

                                            </div>
                                            <div class="nk-block-head-content">
                                                <ul class="nk-block-tools g-3">
                                                    <li><a href="#" class="btn btn-icon search-toggle toggle-search" data-target="search"><em class="icon ni ni-search"></em></a></li>
                                                    <li><a data-bs-toggle="modal" href="#addPlugin" class="btn btn-icon btn-primary d-md-none"><em class="icon ni ni-plus"></em></a>
                                                        <a href="edit/<%= JustSavedData.id %>" class="btn btn-primary d-none d-md-inline-flex"><em class="icon ni ni-plus"></em><span>Edit Article</span></a>
                                                    </li>
                                                </ul>
                                            </div>
                                            <div class="card-search search-wrap border" data-search="search">
